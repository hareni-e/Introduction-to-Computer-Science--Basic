   "source": [
    "def is_prim(n):\n",
    "    is_prime = True\n",
    "    for i in range(2, n-1):\n",
    "        if n % i != 0:\n",
    "            continue\n",
    "        else:\n",
    "            is_prime = False\n",
    "    return is_prime\n",
    "        \n",
    "print(is_prim(8))"
   ]
  }
